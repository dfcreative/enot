{
  "name": "enot",
  "version": "0.1.6",
  "description": "Event NOTation system",
  "main": "index.js",
  "scripts": {
    "build": "npm run min-index && browserify index.min.js --standalone enot > dist/enot.js && npm run minify && npm test",
    "test": "mocha-phantomjs test/index.html",
    "minify": "ccjs dist/enot.js --compilation_level=SIMPLE --language_in=ECMASCRIPT5_STRICT > dist/enot.min.js",
    "min-index": "ccjs index.js --language_in=ECMASCRIPT5_STRICT --compilation_level=ADVANCED --process_jquery_primitives --externs=build/jquery-externs.js > index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dfcreative/enot.git"
  },
  "keywords": [
    "events"
  ],
  "author": "Deema Eevanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/enot/issues"
  },
  "homepage": "https://github.com/dfcreative/enot",
  "devDependencies": {
    "chai": "~1.9.1",
    "mocha": "~1.21.4",
    "require-stub": "*",
    "phantomjs": "~1.9.7-15",
    "mocha-phantomjs": "~3.5.0",
    "browserify": "~5.10.0",
    "closurecompiler": "~1.3.2"
  },
  "dependencies": {
    "matches-selector": "~1.0.0",
    "each-csv": "*",
    "polymer-weakmap": "*",
    "mutypes": "*"
  }
}
