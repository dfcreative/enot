{
  "name": "enot",
  "version": "1.0.20",
  "description": "Event NOTation system",
  "main": "index.js",
  "files": [
    "index.js",
    "Emitter.js",
    "emit.js",
    "on.js",
    "off.js",
    "once.js",
    "src"
  ],
  "scripts": {
    "build-min": "npm run build && npm run min && npm run clean && cat enot.js | gzip-size | pretty-bytes",
    "build": "uncommon ./index -w \";(function(){window['Emitter']=Enot;%output%;})();\" > enot.dev.js",
    "min": "ccjs enot.dev.js --language_in=ECMASCRIPT5 --compilation_level=ADVANCED --process_jquery_primitives --externs=build/jquery-externs.js > enot.js",
    "clean": "rm enot.dev.js",
    "test": "mochify"
  },
  "dependencies": {
    "matches-selector": ">=1.x",
    "each-csv": "*",
    "mutype": "*",
    "mustring": "*",
    "emmy": "*",
    "query-relative": ">=1.x",
    "muparse": ">=0.0.9",
    "sliced": "0.0.5",
    "key-name": ">=1.0.1",
    "parenthesis": ">=1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dfcreative/enot.git"
  },
  "keywords": [
    "events",
    "event",
    "emitter",
    "component",
    "notation"
  ],
  "author": {
    "name": "Deema Yvanow",
    "email": "dfcreative@gmail.com",
    "url": "http://github.com/dfcreative"
  },
  "license": "unlicensed",
  "bugs": {
    "url": "https://github.com/dfcreative/enot/issues"
  },
  "homepage": "https://github.com/dfcreative/enot",
  "devDependencies": {
    "chai": "~1.9.1",
    "mocha": "~1.x",
    "require-stub": "*",
    "phantomjs": "~1.9.7-15",
    "mocha-phantomjs": "~3.5.0",
    "browserify": "5.x",
    "closurecompiler": "~1.x",
    "jquery": "~2.x",
    "testling": "~1.x",
    "polymer-weakmap": "~2.0.0",
    "icicle": "~0.x",
    "mochify": "~2.1.0"
  },
  "testling": {
    "files": "test/index.js",
    "scripts": "node_modules/polymer-weakmap/weakmap.js",
    "harness": "mocha",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  }
}
